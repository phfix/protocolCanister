      <X> 3 </X>
      <Name> TopE </Name>
      <SwitchId> 1 </SwitchId>
      <SwitchAddress> 35 </SwitchAddress>
    </Electrode>

    <Electrode>
      <Id> 4 </Id>
      <X> 4 </X>
      <Name> TopS </Name>
      <SwitchId> 1 </SwitchId>
      <SwitchAddress> 36 </SwitchAddress>
    </Electrode>

    <Electrode>
      <Id> 5 </Id>
      <X> 5 </X>
      <Name> BotW </Name>
      <SwitchId> 1 </SwitchId>
      <SwitchAddress> 37 </SwitchAddress>
    </Electrode>

